{
  "training_data_path": "~/cartpole_training_data.json",
  "state_norm_data_path": "~/cartpole_normalization.json",
  "pytorch_output_path": "~/pytorch_model.pt",
  "use_gpu": true,
  "norm_params": {
    "output_dir": "~/",
    "cols_to_norm": [
      "state_features"
    ],
    "num_samples": 1000
  },
  "actions": [
    "0",
    "1"
  ],
  "epochs": 1,
  "rl": {
    "gamma": 0.99,
    "target_update_rate": 0.2,
    "reward_burnin": 1,
    "maxq_learning": 1,
    "epsilon": 0.2,
    "temperature": 0.35,
    "softmax_policy": 0
  },
  "rainbow": {
    "double_q_learning": false,
    "dueling_architecture": false
  },
  "training": {
    "layers": [
      -1,
      128,
      64,
      -1
    ],
    "activations": [
      "relu",
      "relu",
      "linear"
    ],
    "minibatch_size": 128,
    "learning_rate": 0.001,
    "optimizer": "ADAM",
    "lr_decay": 0.999
  },
  "run_details": {
    "num_episodes": 5001,
    "max_steps": 200,
    "train_every_ts": 1,
    "train_after_ts": 1,
    "test_every_ts": 2000,
    "test_after_ts": 1,
    "num_train_batches": 1,
    "avg_over_num_episodes": 100
  }
}
